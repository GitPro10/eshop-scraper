{
  "name": "eshop-scraper",
  "version": "1.2.1",
  "description": "A simple package to get some data from a e-shop using web-scraping",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "format": "pnpm run format:fix && pnpm run format:check",
    "format:check": "prettier --check --ignore-path .gitignore README.md .",
    "format:fix": "prettier --write --ignore-path .gitignore README.md .",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "bin": "./main.js",
  "type": "module",
  "author": "Jubayer Islam",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.26.0",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21"
  },
  "bugs": {
    "url": "https://github.com/GitPro10/eshop-scraper/issues"
  },
  "homepage": "https://github.com/GitPro10/eshop-scraper#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/GitPro10/eshop-scraper.git"
  },
  "keywords": [
    "price finder",
    "web scrap",
    "eshop",
    "amazon",
    "nintendo",
    "walmart",
    "price fetcher",
    "data fetcher",
    "website data fetcher",
    "e-shop",
    "price tracker"
  ],
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "jest": "^27.5.1",
    "prettier": "^2.7.1"
  }
}
