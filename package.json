{
  "name": "eshop-scraper",
  "version": "1.2.1",
  "description": "A simple package to get some data from a e-shop using web-scraping",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "bin": "./main.js",
  "type": "module",
  "author": "Jubayer Islam",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.26.0",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21",
